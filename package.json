{
  "name": "codebase",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "ramda": "^0.26.1",
    "ramda-adjunct": "^2.19.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest --passWithNoTests",
    "tw": "jest --watch --passWithNoTests",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [">0.2%", "not dead", "not op_mini all"],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "bit": {
    "env": {
      "compiler": "bit.envs/compilers/react-typescript@3.0.6",
      "tester": "lgy87.envs/testers/jest@24.0.4"
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "yarn",
    "resolveModules": {
      "aliases": {
        "~": "src"
      }
    },
    "overrides": {
      "ui/*": {
        "dependencies": {
          "ramda": "^0.26.1"
        }
      }
    }
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.17",
    "@types/jsdom": "^12.2.4",
    "@types/node": "12.7.2",
    "@types/ramda": "^0.26.19",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.8.5",
    "babel-jest": "^24.9.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "jsdom": "^15.1.1",
    "react-scripts": "3.1.1",
    "sinon": "^7.4.1",
    "ts-jest": "^24.0.2",
    "typescript": "3.5.3"
  }
}
