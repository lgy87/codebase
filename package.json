{
  "name": "codebase",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@hot-loader/react-dom": "^16.9.0",
    "ramda": "^0.26.1",
    "ramda-adjunct": "^2.19.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "jest --watch --passWithNoTests",
    "test-nw": "jest --passWithNoTests",
    "eject": "react-app-rewired eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "bit": {
    "env": {
      "compiler": "bit.envs/compilers/react-typescript@3.0.6",
      "tester": "lgy87.envs/testers/jest@24.0.4"
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "yarn",
    "resolveModules": {
      "aliases": {
        "~": "src"
      }
    },
    "overrides": {
      "ui/*": {
        "dependencies": {
          "ramda": "^0.26.1"
        }
      }
    }
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.18",
    "@types/jsdom": "^12.2.4",
    "@types/node": "12.7.2",
    "@types/ramda": "^0.26.19",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.8.5",
    "@typescript-eslint/parser": "^2.0.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.9.0",
    "babel-plugin-lodash": "^3.3.4",
    "customize-cra": "^0.5.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.2.1",
    "eslint-config-react-app": "^5.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "jsdom": "^15.1.1",
    "lodash-webpack-plugin": "^0.11.5",
    "react-app-rewired": "^2.1.3",
    "react-scripts": "3.1.1",
    "sinon": "^7.4.1",
    "ts-jest": "^24.0.2",
    "typescript": "3.5.3",
    "webpack-bundle-analyzer": "^3.4.1"
  }
